<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="399.0" prefWidth="397.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.LoginController">
   <children>
      <AnchorPane prefHeight="399.0" prefWidth="397.0">
         <children>
            <PasswordField fx:id="passwordField" layoutX="95.0" layoutY="148.0" onAction="#passwordHandle" promptText="Password">
               <tooltip>
                  <Tooltip fx:id="ttPassword" text="Empty Tooltip" />
               </tooltip></PasswordField>
            <TextField fx:id="usernameField" layoutX="95.0" layoutY="88.0" onAction="#userHandle" promptText="Username" />
            <Button fx:id="loginButton" layoutX="219.0" layoutY="346.0" mnemonicParsing="false" onAction="#loginHandle" text="Log In" />
            <Button fx:id="menuButton" layoutX="310.0" layoutY="346.0" mnemonicParsing="false" onAction="#menuReturn" text="Menu" />
            <CheckBox fx:id="showPasswordCheck" layoutX="5.0" layoutY="154.0" mnemonicParsing="false" onAction="#showPasswordHandle" prefHeight="13.0" prefWidth="97.0" text="Show password">
               <font>
                  <Font size="10.0" />
               </font>
            </CheckBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
